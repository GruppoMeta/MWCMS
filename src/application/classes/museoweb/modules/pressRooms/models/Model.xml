<?xml version="1.0" encoding="utf-8"?>
<model:Model
    xmlns:glz="http://www.glizy.org/dtd/1.0/"
    xmlns:model="http://www.glizy.org/dtd/1.0/model/"
    model:tableName="museoweb.modules.pressRooms" model:usePrefix="true" model:type="document">

    <model:Define>
        <!-- system -->
        <model:Field name="pressroomdetail_title" type="string" length="255" index="true" />
        <model:Field name="fulltext" type="string" index="fulltext" onlyIndex="true" />

        <!-- custom -->
        <model:Field name="external_id" type="int" length="255" index="true" />
        <model:Field name="pressroomdetail_date" type="date" validator="date,notnull" index="true" />
        <model:Field name="pressroomdetail_author" type="string" length="255" index="true" />
        <model:Field name="pressroomdetail_magazine" type="string" length="255" index="true" />
        <model:Field name="pressroomdetail_url" type="string" />
        <model:Field name="pressroomdetail_category" type="string" length="255" index="true" />
        <model:Field name="pressroomdetail_country" type="string" length="255" index="true" />
        <model:Field name="pressroomdetail_description" type="text" />
        <model:Field name="pressroomdetail_image" type="text" />
        <model:Field name="media" type="object" readFormat="false" />
		<model:Field name="pressroomdetail_thesaurus" type="text" />
    </model:Define>
</model:Model>